{
  "$schema": "https://raw.githubusercontent.com/AArnott/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "1.2",
  "assemblyVersion": {
    "precision": "Build"
  },
  "nugetPackageVersion": {
    "semVer": 2
  },
  "gitCommitIdShortFixedLength": 7,
  "publicReleaseRefSpec": [
    "^refs/heads/master$",
    "^refs/heads/\\d+\\.\\d+$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  }
}
